import { EventEmitter } from '@angular/core';
import '@avl-controls/core/dist/avl-button/avl-button';
export declare class AvlAboutDialogComponent {
    productLogo: string;
    productName: string;
    brandPromise: string;
    versionInfo: VersionInfo[];
    legalText: string;
    additionalInfoLabel: string;
    additionalInfoUrl: string;
    copyright: string;
    okButtonLabel: string;
    private _open;
    set open(value: boolean);
    get open(): boolean;
    openChange: EventEmitter<boolean>;
    /**
     *
     * ### Styling
     * The following custom properties are available for styling:
     *
     * Custom property                                      | Description                             | Default
     * :----------------------------------------------------|:----------------------------------------|:----------------------
     * `--avl-about-dialog-foreground-color`                | Main foreground color                   | `--avl-corporate-foreground-color`
     * `--avl-about-dialog-foreground-medium-color`         | Forground color for version information | `--avl-corporate-foreground-medium-color`
     * `--avl-about-dialog-link-color`                      | Link color                              | `--avl-corporate-link-color`
     * `--avl-about-dialog-link-hover-color`                | Link hover color                        | `--avl-corporate-link-hover-color`
     * `--avl-about-dialog-on-button-background-color`      | Button text color                       | `--avl-corporate-on-button-background-color`
     * `--avl-about-dialog-button-background-color`         | Button background color                 | `--avl-corporate-button-background-color`
     * `--avl-about-dialog-button-background-hover-color`   | Button hover color                      | `--avl-corporate-button-background-hover-color`
     * `--avl-about-dialog-button-background-focus-color`   | Button focus color                      | `--avl-corporate-button-background-focus-color`
     * `--avl-about-dialog-button-background-pressed-color` | Button pressed color                    | `--avl-corporate-button-background-pressed-color` *
     */
    _styling: any;
    constructor();
    closeDialog(): void;
    /** @hidden */
    openChanged(value: boolean): void;
}
export interface VersionInfo {
    label: string;
    value: string;
}

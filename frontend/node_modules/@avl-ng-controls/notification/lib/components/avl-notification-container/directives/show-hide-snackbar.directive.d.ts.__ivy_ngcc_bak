import { SimpleChanges, OnChanges, ElementRef } from '@angular/core';
/**@hidden */
export declare class SnackbarMethodDescription {
    method: 'show' | 'hide';
    animate: boolean;
    constructor(method: 'show' | 'hide', animate: boolean);
}
export declare class ShowHideSnackbarDirective implements OnChanges {
    /** @hidden */
    methodDescription: SnackbarMethodDescription;
    private element;
    constructor(el: ElementRef);
    ngOnChanges(changes: SimpleChanges): void;
}

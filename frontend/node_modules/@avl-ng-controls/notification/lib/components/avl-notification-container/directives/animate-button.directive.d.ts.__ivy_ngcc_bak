import { SimpleChanges, OnChanges } from '@angular/core';
export declare class AnimateButton implements OnChanges {
    /** @hidden */
    increment: number;
    /** @hidden */
    infoStackHeight: number;
    /** @hidden */
    warningStackHeight: number;
    /** @hidden */
    infoStackIndicatorHeight: number;
    /** @hidden */
    warningStackIndicatorHeight: number;
    styleTop: string;
    constructor();
    ngOnChanges(changes: SimpleChanges): void;
    private getInfoHeight;
    private getWarningHeight;
    private getOffsetHeight;
}

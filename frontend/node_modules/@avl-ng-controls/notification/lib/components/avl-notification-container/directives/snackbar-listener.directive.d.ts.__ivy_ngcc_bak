import { EventEmitter } from '@angular/core';
export declare class SnackbarListener {
    /** @hidden */
    notificationClicked: EventEmitter<Event>;
    /** @hidden */
    buttonClicked: EventEmitter<Event>;
    /** @hidden */
    notificationDismissed: EventEmitter<Event>;
    onNotificationClicked(event: Event): void;
    onButtonClicked(event: Event): void;
    onNotificationDismissed(event: Event): void;
}

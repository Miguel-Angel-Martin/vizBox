#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@avl-vizbox-develop/i18n-tools/dist/po2json.js" "$@"
  ret=$?
else 
  node  "$basedir/../@avl-vizbox-develop/i18n-tools/dist/po2json.js" "$@"
  ret=$?
fi
exit $ret

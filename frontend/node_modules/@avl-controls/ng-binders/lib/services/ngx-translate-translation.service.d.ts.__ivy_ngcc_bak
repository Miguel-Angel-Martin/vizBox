import { AvlTranslationService } from '@avl-controls/interfaces/src/avl-translation-service';
import { TranslateService } from '@ngx-translate/core';
export declare class NgxTranslateTranslationService implements AvlTranslationService {
    private translateService;
    get language(): string;
    constructor(translateService: TranslateService);
    translate(key: string, interpolateParams?: object): Promise<string>;
}

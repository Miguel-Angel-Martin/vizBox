import { EventEmitter, ElementRef } from '@angular/core';
export declare class AvlDatatableDirective {
    buttonClick: EventEmitter<any>;
    overflowMenuItemClick: EventEmitter<any>;
    linkClick: EventEmitter<any>;
    pageSizeChange: EventEmitter<number>;
    selectedPageChange: EventEmitter<number>;
    constructor(elementRef: ElementRef);
    buttonClickListener(e: CustomEvent): void;
    overflowMenuItemClickListener(e: CustomEvent): void;
    linkClickListener(e: CustomEvent): void;
}

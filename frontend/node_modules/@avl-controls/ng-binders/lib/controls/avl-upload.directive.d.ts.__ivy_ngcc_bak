import { ElementRef, OnInit, EventEmitter } from '@angular/core';
import { AvlFile, AvlUploadService } from '@avl-controls/interfaces';
export declare class AvlUploadDirective implements OnInit {
    private element;
    private uploadService;
    uploadComplete: EventEmitter<AvlFile>;
    uploadAbort: EventEmitter<AvlFile>;
    uploadFail: EventEmitter<any>;
    uploadStart: EventEmitter<AvlFile>;
    filesChanged: EventEmitter<AvlFile[]>;
    fileRejected: EventEmitter<AvlFile>;
    menuItemClicked: EventEmitter<any>;
    downloadFail: EventEmitter<any>;
    downloadAbort: EventEmitter<AvlFile>;
    constructor(element: ElementRef, uploadService: AvlUploadService);
    ngOnInit(): void;
    uploadCompleteHandler(e: CustomEvent): void;
    uploadCanceledHandler(e: CustomEvent): void;
    uploadStartedHandler(e: CustomEvent): void;
    uploadFailedHandler(e: CustomEvent): void;
    filesChangedHandler(e: CustomEvent): void;
    fileRejectedHandler(e: CustomEvent): void;
    menuItemClickedHandler(e: CustomEvent): void;
    downloadFailedHandler(e: CustomEvent): void;
    downloadCanceledHandler(e: CustomEvent): void;
}

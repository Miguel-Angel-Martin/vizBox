    ], AvlCheckboxDirective.prototype, "indeterminateChange", void 0);
    AvlCheckboxDirective = __decorate([
        Directive({
            selector: 'avl-checkbox'
        }),
        __metadata("design:paramtypes", [ElementRef])
    ], AvlCheckboxDirective);

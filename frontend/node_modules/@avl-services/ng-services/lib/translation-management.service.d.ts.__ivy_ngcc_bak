import { TranslateService } from '@ngx-translate/core';
import { NgxTranslateTranslationService } from '@avl-controls/ng-binders';
import '@avl-controls/core/dist/avl-localization-service/avl-localization-service';
export declare class TranslationManagementService {
    private translateService;
    private translationService;
    constructor(translateService: TranslateService, translationService: NgxTranslateTranslationService);
    languages: {
        code: string;
        label: string;
    }[];
    initializeTranslations(): () => Promise<any>;
    get language(): string;
    changeLanguage(lang: string): void;
}
